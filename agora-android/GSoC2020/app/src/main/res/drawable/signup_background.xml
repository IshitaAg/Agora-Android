<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorYellow"/>
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="55" android:toDegrees="45" android:pivotX="-92%" android:pivotY="27%" >
            <shape android:shape="rectangle">
                <stroke android:color="@color/colorWhite" android:width="10dp"/>
                <solid android:color="@color/colorWhite" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="46" android:toDegrees="45" android:pivotX="120%" android:pivotY="4%" >
            <shape android:shape="rectangle" >
                <stroke android:color="@color/colorWhite" android:width="10dp"/>
                <solid android:color="@color/colorWhite" />
            </shape>
        </rotate>
    </item>
</layer-list>